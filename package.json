{
    "name": "ApiPrestadores",
    "version": "1.0.0",
    "description": "Api Rest prestadores de ioma (medicos, farmacias, instituciones y establecimientos)",
    "main": "index.js",
    "scripts": {
        "start": "nodemon index.js --exec babel-node",
        "cleanMac": "rm -rf build && mkdir build",
        "cleanWin": "del build && mkdir build",
        "build": "babel ./ --out-dir ./build --ignore node_modules,test,loggin",
        "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/*.test.js --ui bdd --reporter spec --colors components --recursive --compilers js:babel-register --require babel-polyfill --timeout 15000 --exit",
        "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors components --recursive --compilers js:babel-register --require babel-polyfill --timeout 15000 --exit"
    },
    "keywords": [],
    "author": {
        "name": "Alejo Bianchi",
        "email": "alejobianchi@gmail.com"
    },
    "license": "ISC",
